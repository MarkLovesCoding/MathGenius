<div class="game-container display-container" id="game-container">
  <div class="top game-top">
    <div class="titles-group game-titles-group">
      <div class="titles-left">
        <div class="activity-title-container">

        <h2 class="activity-title">High Score Challenge</h2>
        <div class="title-icon"><i class=" game-icon icon-small icon fas fa-crown fa-large"></i>
        </div>
          </div>  
          <div class="selections-container">
            <div class="selected-operator-container" id="selected-operator-container-general">
              <div class="selected-operator" id="selected-operator-text-general">
              </div>
              <!-- <i class="selected-operator-icon " id="selected-operator-icon-general"></i> -->
            </div>
            <div class="selected-difficulty-container" id="selected-difficulty-container-general">
              <div id="selected-difficulty-general"></div>
            </div>
          </div>


      </div>

    </div>
  </div>
  <div class="game-middle-inner">
    <div class="game-middle-lower">
      <div class="game-box">
        <form id="game-answer-form" class="question-box game-questiion-box">
          <div class="question-field game-question">
            <div id="game-first-number" class="question-number question-text"></div>
            <div id="game-first-operator" class="operator question-text"></div>
            <div id="game-second-number" class="question-number question-text"></div>
            <div id="game-second-operator" class="operator question-text">=</div>

            <input type="number" min="0" id="game-answer-input" class="input game-input" placeholder="?"
              autocomplete="off"></input>
          </div>
          <button type="submit" id="answer-submit-btn" class="answer-submit">Check</button>
        </form>
      </div>
      <div class="game-lower-landscape">
        <div class="game-lower">
          <div class="correctness-container ">
            <div id="game-correctness" class="correctness game-correctness"></div>
            <div class="actual-container hidden" id="game-actual-container">
              <div class="actual-text">The correct answer was:</div>
              <div class="actual" id="game-actual"></div>
            </div>
          </div>
          <div class="score-container">
            <div class="curr-score-container">
              <div class="score-title">Current Score:</div>
              <div id="game-current-score" class="score-text"> 0</div>
            </div>
            <div class="high-score-container">
              <div class="high-title">High Score:</div>
              <div id="game-current-high" class="score-text">0</div>
            </div>
          </div>
        </div>
        <div class="progress-container">
          <div class="level-container">
            <div class="level-text">Level</div>
            <div class="level-number" id="game-level-number">0</div>
          </div>
          <div class="tracker-container" id="game-tracker-container">
            <div class="tracker" id="game-tracker"></div>
          </div>
          <div class="tracker-container" id="game-tracker-container-2">
            <div class="tracker" id="game-tracker-2"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>