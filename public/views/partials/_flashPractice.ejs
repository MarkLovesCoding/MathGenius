<div
  class="flash-container sub-container display-container"
  id="flash-container"
>
  <div class="flash-upper">
    <div class="titles-group">
      <div class="titles-left">
        <div class="activity-title-container">
          <h2 id="activity-title-flash" class="activity-title">
            Flash Cards Practice
          </h2>

          <div class="title-icon">
            <i class="flash-icon icon-small fas fa-bolt fa-large"></i>
          </div>
        </div>
        <div class="selections-container">
          <div
            class="selected-operator-container"
            id="selected-operator-container-general"
          >
            <div
              class="selected-operator"
              id="selected-operator-text-general"
            ></div>
            <!-- <i class="selected-operator-icon " id="selected-operator-icon-general"></i> -->
          </div>
          <div
            class="selected-difficulty-container"
            id="selected-difficulty-container-general"
          >
            <div id="selected-difficulty-general"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="flash-card-container middle flash-middle" id="flash-card">
    <div id="flash-question-box" class="flash-box flash-question-box">
      <div id="flash-first-number" class="question-number flash-text"></div>
      <div id="flash-first-operator" class="operator flash-text"></div>
      <div id="flash-second-number" class="question-number flash-text"></div>
    </div>
    <div class="flash-box flash-answer-box" id="flash-answer-box">
      <div class="flash-answer flash-text" id="flash-answer"></div>
    </div>
  </div>
</div>
